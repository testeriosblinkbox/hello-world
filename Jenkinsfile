pipeline{
    agent any
    
    environment{
        PATH = ""
    }
    stages{
       
        stage("Git Checkout"){
            steps{
                  echo 'welcome to build'
            }
        }
       
        stage("Test"){
            steps{
                   echo 'welcome to test'
            }
        }
     
      stage("Deploy"){
            steps{
                   echo 'Welcome to Deploy'
            }
        }
   }
}
